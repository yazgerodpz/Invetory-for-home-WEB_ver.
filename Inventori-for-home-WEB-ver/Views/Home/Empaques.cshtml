
@{
    ViewData["Title"] = "Empaques";
}


<title>Tabla de tipos de empaques</title>
<style>
    body {
        font-family: Arial, sans-serif;
    }
    table {
        width: 100%;
        border-collapse: collapse;
    }
    th, td {
        border: 1px solid #ddd;
        padding: 8px;
        text-align: left;
    }
    th {
        background-color: #f2f2f2;
    }
    tr:nth-child(even) {
        background-color: #f9f9f9;
    }
    tr:hover {
        background-color: #f1f1f1;
    }
</style>
<body>
    <h1>Tipos de empaques</h1>
    <button onclick="llenarTabla()">LLENAR TABLA</button>
    <button onclick="showAlerta()">MOSTRAR ALERTA</button>
    <table id="gridEmpaques">
        <thead>
            <tr>
                <th>ID</th>
                <th>Tipos de empaques</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>1</td>
                <td>Caja de 24 pza</td>
            </tr>
        </tbody>
    </table>
</body>

